@article{bodinConservationSuccessFunction2014,
  title = {Conservation {{Success}} as a {{Function}} of {{Good Alignment}} of {{Social}} and {{Ecological Structures}} and {{Processes}}: {{Social-Ecological Fit}} and {{Conservation}}},
  shorttitle = {Conservation {{Success}} as a {{Function}} of {{Good Alignment}} of {{Social}} and {{Ecological Structures}} and {{Processes}}},
