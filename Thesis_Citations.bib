@article{hebblewhiteWolvesBordersTransboundary2020,
  title = {Wolves without Borders: {{Transboundary}} Survival of Wolves in {{Banff National Park}} over Three Decades},
  shorttitle = {Wolves without Borders},
  author = {Hebblewhite, Mark and Whittington, Jesse},
