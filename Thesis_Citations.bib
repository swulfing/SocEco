@article{jiaMappingGlobalDistribution2023,
  title = {Mapping Global Distribution of Mangrove Forests at 10-m Resolution},
  author = {Jia, Mingming and Wang, Zongming and Mao, Dehua and Ren, Chunying and Song, Kaishan and Zhao, Chuanpeng and Wang, Chao and Xiao, Xiangming and Wang, Yeqiao},
