@article{gervasiCompensatoryImmigrationCounteracts2015,
  title = {Compensatory Immigration Counteracts Contrasting Conservation Strategies of Wolverines ( {{Gulo}} Gulo ) within {{Scandinavia}}},
  author = {Gervasi, Vincenzo and Br{\o}seth, Henrik and Nilsen, Erlend B. and Ellegren, Hans and Flagstad, {\O}ystein and Linnell, John D.C.},
