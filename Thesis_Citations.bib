  author = {Zu Ermgassen, Philine S.E. and Mukherjee, Nibedita and Worthington, Thomas A. and Acosta, Alejandro and Rocha Araujo, Ana Rosa Da and Beitl, Christine M. and {Castellanos-Galindo}, Gustavo A. and {Cunha-Lignon}, Mar{\'i}lia and {Dahdouh-Guebas}, Farid and Diele, Karen and Parrett, Cara L. and Dwyer, Patrick G. and Gair, Jonathan R. and Johnson, Andrew Frederick and Kuguru, Baraka and Savio Lobo, Aaron and Loneragan, Neil R. and {Longley-Wood}, Kate and Mendon{\c c}a, Jocemar Tomasino and Meynecke, Jan-Olaf and Mandal, Roland Nathan and Munga, Cosmas Nzaka and Reguero, Borja G. and R{\"o}nnb{\"a}ck, Patrik and Thorley, Julia and Wolff, Matthias and Spalding, Mark},
